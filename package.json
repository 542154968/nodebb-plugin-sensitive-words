  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-angular": "19.5.0",
    "eslint": "9.x",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.x",
    "husky": "9.1.6",
    "lint-staged": "15.2.10"
  }
